{
    "name": "dkd/contentdashboard",
    "type": "typo3-cms-extension",
    "description": "TYPO3 Extension Content Dashboard",
    "keywords": ["cmis", "content dashboard", "typo3"],
    "homepage": "https://phabricator.dkd.de/diffusion/EXTCD/",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Sascha Egerer",
            "homepage": "http://www.dkd.de",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Dkd\\Contentdashboard\\": "Classes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dkd\\Contentdashboard\\Tests\\": "Tests/"
        },
        "classmap": [
            "typo3/sysext/core",
            "typo3/sysext/backend",
            "typo3/sysext/frontend",
            "typo3/sysext/extbase",
            "typo3/sysext/fluid",
            "typo3/sysext/lang"
        ]
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "1.*",
        "phpunit/phpunit": "~4.4.5",
        "mikey179/vfsStream": "~1.4.0",
        "typo3-ci/typo3sniffpool": "*",
        "typo3-ci/typo3cms": "*",
        "typo3/cms": "~6.2.9"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "http://composer.typo3.org"
        }
    ]
}
